{
  "name": "runebook",
  "productName": "RuneBook",
  "version": "1.9.2",
  "description": "An automatic rune pages manager for League of Legends",
  "main": "./src/main.js",
  "license": "MIT",
  "repository": "https://github.com/Soundofdarkness/RuneBook",
  "author": {
    "name": "OrangeNote",
    "url": "https://github.com/OrangeNote"
  },
  "scripts": {
    "test": "xo",
    "start": "electron ./src/main.js",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -p onTagOrDraft --win nsis portable",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win",
    "dist:all": "electron-builder -mw -p onTagOrDraft"
  },
  "build": {
    "win": {
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "atob": "^2.1.2",
    "cheerio": "^1.0.0-rc.3",
    "electron-debug": "^3.1.0",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1 ",
    "electron-window-state": "^5.0.3",
    "freezer-js": "^0.14.1",
    "glob": "^7.1.6",
    "html5sortable": "^0.9.17",
    "jquery": "^3.5.1",
    "lcu-connector": "^2.1.3",
    "lodash": "^4.17.15",
    "popper.js": "^1.14.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "riot": "^3.13.2",
    "riot-i18n": "^0.2.1",
    "semantic-ui-css": "~2.3.0",
    "tether": "^1.4.7",
    "username": "^5.1.0",
    "winston": "3.2.1",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "electron-packager": "^14.2.1",
    "xo": "^0.30.0"
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ]
  }
}
